[sub_resource type="GDScript" id="GDScript_ae00b"]
script/source = "extends Control

@onready var home_button: TouchScreenButton = $CanvasLayer/HomeButton
@onready var ovkills: Label = $CanvasLayer/TextureRect/OverallKills
@onready var best_run: Label = $CanvasLayer/TextureRect/FastestTime
@onready var time_survived: Label = $CanvasLayer/TextureRect/LongestTimeSurvived
@onready var overall_cash: Label = $CanvasLayer/TextureRect/OverallCash

func on_ready():
	if DisplayServer.is_touchscreen_available() and OS.has_feature(\"Andriod\"):
		get_viewport().screen_set_orientation(DisplayServer.SCREEN_PORTRAIT)
	else:
		return

func _process(_delta) -> void:
	ovkills.text = \"OVERALL KILLS: 
					%d\" % GameManager.total_kills

	best_run.text = \"FASTEST LEGACY PROTOCOL RUN: 
					%d\" % GameManager.quickest_time

	time_survived.text = \"LONGEST TIME LASTED IN HYPERCORE GAUNTLET: 
					%d\" % GameManager.longest_survival

	overall_cash.text = \"MONEY IN THE BANK: 
							$ %d\" % GameManager.total_cash




func _on_home_button_pressed() -> void:
	get_tree().change_scene_to_file(\"res://Scenes/MainMenu.tscn\")
"

